@using ProjectDunyam.ViewModels
@model IEnumerable<Project>
@{
    ViewData["Title"] = "Home Page";
}

<h2>Projeler</h2>
<table>
    <tr>
        <th>ID</th>
        <th>Başlık</th>
        <th>Açıklama</th>
    </tr>
    @foreach (var project in Model)
    {
        <tr>
            <td>@project.ProjectId</td>
            <td>@project.Title</td>
            <td>@project.Description</td>
        </tr>
    }
</table>